<!DOCTYPE html>
<html>
<head>
  <$- include('../partials/head') $>
</head>
<body>
  <$- include('../partials/header', { subHeading: user.name + "'s Profile" }) $>

  <main>

    <p>Name:   <$= user.name $></p>
    <p>Handle: <$= user.handle $></p>

    <div class="showContainer">

      <div id="mainImage">
          <img class="showImage" src="/images/bench-nature.jpg" />
      </div>

      <div class="imageDetails">
        <div id="showDetails">
          <$= user.name $>
          <$= user.spots[0].description$>
          <$= user.spots[0]$>


          <br>
        </div>

        <div id="showMap">
          <img class="showImageMap" src="/images/map.jpeg" />
        </div>
      </div>
    </div>

  </main>

  <$- include('../partials/scripts') $>
</body>
</html>
